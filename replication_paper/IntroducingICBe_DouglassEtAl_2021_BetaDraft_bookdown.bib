}

@article{yarhi-miloArmAllyPatron2016,
	title = {To {Arm} or to {Ally}? {The} {Patron}'s {Dilemma} and the {Strategic} {Logic} of {Arms} {Transfers} and {Alliances}},
	volume = {41},
